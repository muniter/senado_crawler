{
  "name": "crawler",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "reset": "rm db/database.db && npm run db:migrate && npm run db:seed && npm run db:gen-ts-schema",
    "test": "jest",
    "format": "prettier --write \"src/**/*.ts\"",
    "db:gen-ts-schema": "pnpx kysely-codegen --out-file src/database/schema.ts",
    "db:migrator": "pnpx tsx src/database/migrator.ts",
    "db:migrate": "pnpx tsx src/database/migrator.ts --action migrate",
    "db:seed": "pnpx tsx src/database/seeder.ts",
    "build": "tsc --build",
    "refresh": "tsx ./src/utils/refresh.ts",
    "gen-reports": "tsx ./src/utils/gen-report.ts",
    "gen-fixtures": "tsx ./src/utils/generate-fixture-results.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.5",
    "better-sqlite3": "^9.2.2",
    "cheerio": "^1.0.0-rc.12",
    "kysely": "^0.27.2",
    "p-queue": "^8.0.1",
    "pg": "^8.13.0",
    "pino": "^9.0.0",
    "pino-pretty": "^11.0.0",
    "remeda": "^1.36.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.8",
    "@types/google-spreadsheet": "^3.3.2",
    "@types/jest": "^29.5.11",
    "@types/node": "^18.11.9",
    "@types/pg": "^8.11.10",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "commander": "^11.1.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "kysely-codegen": "^0.11.0",
    "prettier": "3.2.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
